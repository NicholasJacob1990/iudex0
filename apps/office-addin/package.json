{
  "name": "@iudex/office-addin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3100",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "validate-manifest": "npx office-addin-manifest validate manifest.xml",
    "sideload": "npx office-addin-dev-certs install && npx office-addin-debugging start manifest.xml"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.27.0",
    "@fluentui/react-components": "^9.56.0",
    "@fluentui/react-icons": "^2.0.260",
    "axios": "^1.6.5",
    "diff": "^8.0.3",
    "dompurify": "^3.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@iudex/shared": "*",
    "@types/dompurify": "^3.2.0",
    "@types/diff": "^7.0.2",
    "@types/office-js": "^1.0.421",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.0",
    "office-addin-dev-certs": "^1.13.4",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.14"
  }
